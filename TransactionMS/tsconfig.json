{
  "compilerOptions": {
    "target": "es2020",             /* Specify ECMAScript target version */
    "module": "commonjs",           /* Specify module code generation */
    "outDir": "./dist",             /* Redirect output structure to the directory */
    "rootDir": "./src",             /* Specify the root directory of input files */
    "strict": true,                 /* Enable all strict type-checking options */
    "esModuleInterop": true,         /* Enable/disable emitting use of `import` & `export` */
    "skipLibCheck": true,           /* Skip type checking of all declaration files */
    "forceConsistentCasingInFileNames": true /* Ensure that casing is consistent in file names */

    /* Other options you might want to consider based on your project needs:
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    },
    "allowJs": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "declaration": true,
    "noImplicitAny": false,
    */
  },
  "include": [
    "src/**/*.ts"
, "src/data/import_dev_data.js"  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
